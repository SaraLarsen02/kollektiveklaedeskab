---
import MainLayout from "../layouts/MainLayout.astro";
import Button from "../components/Button.astro";
import Link from "../components/Link.astro";
import { Image } from "astro:assets";
import thumbsup from "../assets/images/thumbsup.png";
import os from "../assets/images/os_1.webp";
---

<MainLayout title="Titel">
  <article>
    <h1>Index overskrift</h1>
    <Button>Klik!</Button>
    <p>Link til <Link href="https://www.kea.dk" target="_blank">En anden side</Link></p>
    <Image src={thumbsup} alt="" />
  </article>
  <h2>Testetstets</h2>
  <img src="/butik_1.webp" alt="" />
  <h2>vi pråver igen</h2>
  <Image class="" src={os} alt="Blå bue" />
</MainLayout>
